{
  "name": "@haught/bench-svelte",
  "version": "1.0.1",
  "description": "A simple table package for Svelte using remote data",
  "main": "src/index.js",
  "scripts": {
    "dev": "vite dev --open --host=0.0.0.0",
    "prebuild": "node scripts/index.js && pnpm format",
    "build": "vite build",
    "preview": "vite preview",
    "package": "svelte-package",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "lint": "eslint .",
    "format": "prettier --write ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.ncsu.edu/ncstate-comtech/bench-svelte"
  },
  "keywords": [
    "table",
    "pagination",
    "search",
    "sort",
    "svelte"
  ],
  "author": "Matt Haught",
  "license": "MIT",
  "bugs": {
    "url": "https://github.ncsu.edu/ncstate-comtech/bench-svelte/issues"
  },
  "homepage": "https://github.ncsu.edu/ncstate-comtech/bench-svelte#readme",
  "exports": {
    ".": {
      "svelte": "./src/index.js"
    }
  },
  "svelte": "./src/index.js",
  "peerDependencies": {
    "svelte": "^3.55.1 || ^4.0.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "@sveltejs/adapter-vercel": "^4.0.4",
    "@sveltejs/kit": "^2.3.0",
    "@sveltejs/package": "2.2.5",
    "@sveltejs/vite-plugin-svelte": "^3.0.1",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.35.1",
    "mdsvex": "^0.11.0",
    "prettier": "^3.1.1",
    "prettier-plugin-svelte": "^3.1.2",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-slug": "^6.0.0",
    "svelte": "^4.2.8",
    "svelte-check": "^3.6.2",
    "svelte-preprocess": "^5.1.3",
    "svelte2tsx": "^0.6.27",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.11"
  },
  "type": "module"
}
